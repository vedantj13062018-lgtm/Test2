{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "node": "20.18.0"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "withoutCredentials": true
      },
      "node": "20.18.0"
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "node": "20.18.0"
    }
  },
  "submit": {
    "production": {}
  }
}
